{
    "name": "jonston/clever-bot",
    "description": "Laravel AI Agent package with multi-model support",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.5",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "CleverBot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CleverBot\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CleverBot\\CleverBotServiceProvider"
            ],
            "aliases": {
                "CleverBot": "CleverBot\\Facades\\CleverBot"
            }
        }
    },
    "minimum-stability": "stable"
}
